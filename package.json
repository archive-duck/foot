{
  "name": "duck-loader",
  "private": true,
  "version": "1.9.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && tsc -p tsconfig.scripts.json",
    "minify:scripts": "find ./dist/scripts -type f -name \"*.js\" -exec uglifyjs {} -c -m --toplevel --output {} \\; -print",
    "prebuild": "tsc --noEmit && tsc --noEmit -p tsconfig.scripts.json && node ./scripts/prebuild.js",
    "postbuild": "node ./scripts/postbuild.js && npm run minify:scripts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "chakra-react-select": "^4.8.0",
    "framer-motion": "^11.2.6",
    "lodash": "^4.17.21",
    "pdfjs-dist": "^4.3.136",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.5",
    "react-icons": "^5.2.1",
    "uglify-js": "^3.18.0",
    "wouter": "^3.2.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.268",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
